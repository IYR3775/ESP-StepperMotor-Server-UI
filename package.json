{
    "name": "esp-steppermotor-server-ui",
    "version": "0.1.1",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build && npm run prepareExampleDistPackageForESP",
        "lint": "vue-cli-service lint",
        "prepareExampleDistPackageForESP": "rm -rf data && mkdir data && cp -R dist/* data/ && rm -rf data/js/*.js && rm -rf data/js/*.map* && sed -i '' \"s/app\\.js/app\\.js\\.gz/g\" data/index.html && sed -i '' \"s/#VERSION#/$npm_package_version/g\" data/index.html"
    },
    "dependencies": {
        "core-js": "^2.6.5",
        "vue": "^2.6.6",
        "vue-router": "^3.0.1",
        "toastr": "^2.1.4",
        "vue-toastr-2": "0.0.10",
        "bootstrap-vue": "^2.0.0-rc.18",
        "bootstrap": "^4.3.1",
        "@fortawesome/fontawesome-svg-core": "^1.2.17",
        "@fortawesome/free-solid-svg-icons": "^5.8.1",
        "@fortawesome/vue-fontawesome": "^0.1.6",
        "axios": "^0.18.0"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.5.0",
        "@vue/cli-plugin-eslint": "^3.5.0",
        "@vue/cli-service": "^3.5.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.8.0",
        "eslint-plugin-vue": "^5.0.0",
        "vue-template-compiler": "^2.5.21",
        "compression-webpack-plugin": "^4.0.0"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}